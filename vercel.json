{
  "version": 2,
  "builds": [{ 
    "src": "index.js", 
    "use": "@vercel/node",
    "config": {
      "buildCommand": "npm run build"
    }
  }],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "https://e-helpdesk-front.vercel.app/api/auth/$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "buildCommand": "npm run build"
}